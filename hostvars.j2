# example of how to get the ipaddress of every machine
# for use in a template

{% for host in ansible_play_hosts %}
  HOST: {{ host }} IP: {{ hostvars[host]['ansible_default_ipv4']['address'] }}
  HOST: {{ host }} V-38668: {{ hostvars[host]['V-38668-RESULTS']['changed'] | default('NO-V-38668-RESULTS') }}
{% endfor %}

